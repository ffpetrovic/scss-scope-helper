{
	"name": "scss-scope-helper",
	"displayName": "SCSS Scope Helper",
	"description": "Add helpers next to the closing bracket of a scss block",
	"version": "0.0.1",
	"publisher": "ffpetrovic",
	"icon": "art/logo.png",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:scss"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "scss-scope-helper.hide",
				"title": "SCSS Scope Helper: Hide temporarily"
			},
			{
				"command": "scss-scope-helper.show",
				"title": "SCSS Scope Helper: Show"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.12.21",
		"eslint": "^5.13.0",
		"typescript": "^3.3.1",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"scss-parser": "^1.0.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ffpetrovic/scss-scope-helper.git"
	}
}
